{
  "name": "@epfl-si/appauth",
  "version": "0.10.0",
  "description": "@openid/appauth made simple (and GDPR-compliant)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "set -e -x; export NODE_ENV=production; babel --root-mode upward src/* --out-dir dist --extensions '.ts,.tsx' --source-maps; tsc --project tsconfig.build.json",
    "apidocs": "set -e -x; jsdoc2md --files src/OpenIDConnect.tsx src/sundry-widgets.tsx --configure jsdoc2md.json > API.md"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epfl-si/react-npm-modules.git"
  },
  "keywords": [
    "OpenID-Connect",
    "OAuth2",
    "@openid/appauth"
  ],
  "author": "EPFL ISAS-FSD <isas-fsd@groupes.epfl.ch>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/epfl-si/react-npm-modules/issues"
  },
  "homepage": "https://github.com/epfl-si/react-npm-modules#readme",
  "dependencies": {
    "@openid/appauth": "^1.3.1",
    "core-js": "^3.25.5",
    "resolvable-promise": "^2.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
